        "type": "object",
        "required": [
          "batch_id",
          "status",
          "total_files",
          "processed",
          "successful",
          "failed",
          "created_at",
          "message"
        ],
        "title": "BatchJobResponse",
        "description": "Réponse de job batch",
        "example": {
          "batch_id": "batch_xyz789abc",
          "created_at": "2025-01-15T10:30:00Z",
          "failed": 0,
          "message": "Traitement en cours: 4/7 fichiers",
          "processed": 4,
          "status": "processing",
          "successful": 4,
          "total_files": 7
        }
      },
      "BatchResultsResponse": {
        "properties": {
          "batch_id": {
            "type": "string",
            "title": "Batch Id"
          },
          "status": {
            "$ref": "#/components/schemas/JobStatus"
          },
          "total_files": {
            "type": "integer",
            "title": "Total Files"
          },
          "successful": {
            "type": "integer",
            "title": "Successful"
          },
          "failed": {
            "type": "integer",
            "title": "Failed"
          },
          "total_time": {
            "type": "number",
            "title": "Total Time"
          },
          "results": {
            "items": {
              "$ref": "#/components/schemas/BatchFileResult"
            },
            "type": "array",
            "title": "Results"
          }
        },
        "type": "object",
        "required": [
          "batch_id",
          "status",
          "total_files",
          "successful",
          "failed",
          "total_time",
          "results"
        ],
        "title": "BatchResultsResponse",
        "description": "Résultats détaillés d'un batch",
        "example": {
          "batch_id": "batch_xyz789abc",
          "failed": 0,
          "results": [
            {
              "filename": "DOSSIER_18236.pdf",
              "metrics": {},
              "output_file": "DOSSIER_18236.xml",
              "processing_time": 0.64,
              "success": true
            }
          ],
          "status": "completed",
          "successful": 7,
          "total_files": 7,
          "total_time": 3.64
        }
      },
